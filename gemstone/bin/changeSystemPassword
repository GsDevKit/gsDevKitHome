#!/bin/bash

topaz -l <<EOF

set gemstone seaside
set user DataCurator pass swordfish

login
run
System myUserProfile oldPassword: 'swordfish' newPassword: 'newpassword'.
%

commit
logout

set user SystemUser pass swordfish
login
run
System myUserProfile oldPassword: 'swordfish' newPassword: 'newpassword'.
%

commit
logout
errorCount

EOF

